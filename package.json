  "description": "",
  "dependencies": {
    "@11ty/eleventy": "^3.1.2",
    "@fortawesome/fontawesome-free": "^7.0.0",
    "bootstrap-icons": "^1.13.1",
    "luxon": "^3.7.1",
    "tinacms": "^2.8.2"
  },
  "devDependencies": {
    "@tailwindcss/cli": "^4.1.11",
    "@tailwindcss/typography": "^0.5.16",
    "@tinacms/cli": "^1.10.2",
    "@types/node": "^24.2.0",
    "autoprefixer": "^10.4.21",
    "daisyui": "^5.0.50",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.11"
